<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="manutair2.FXMLclienteController">
    <children>
      <Label id="mainLabel" fx:id="mainLabel" layoutX="85.0" layoutY="14.0" text="Bem-vindo à MultAir!">
         <font>
            <Font size="16.0" />
         </font>
      </Label>
      <Label fx:id="label" layoutX="93.0" layoutY="55.0" text="Já tem contrato conosco?" />
      <Button fx:id="botaoNao" layoutX="77.0" layoutY="114.0" mnemonicParsing="false" onAction="#handleBotaoNao" text="Não" />
      <Button fx:id="botaoSim" layoutX="204.0" layoutY="114.0" mnemonicParsing="false" onAction="#handleBotaoSim" text="Sim" />
      <Button fx:id="voltar" layoutX="14.0" layoutY="1.0" mnemonicParsing="false" onAction="#handleBotaoVoltar" text="Voltar" />
    </children>
</AnchorPane>
