<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="200" prefWidth="320" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="manutair2.FXMLacoesClienteController">
    <children>
      <Button fx:id="cadastrarEquipamento" layoutX="100.0" layoutY="77.0" mnemonicParsing="false" onAction="#hadleBotaoCadastrarEquipamento" text="Cadastrar novo equipamento" />
      <Label id="mainLabel" fx:id="mainLabel" alignment="CENTER" layoutY="28.0" prefHeight="49.0" prefWidth="363.0" text="Escolha uma das ações abaixo:" textAlignment="RIGHT" wrapText="true" />
      <Button fx:id="inicio" layoutX="303.0" layoutY="1.0" mnemonicParsing="false" onAction="#handleBotaoInicio" prefHeight="16.0" prefWidth="45.0" text="Início">
         <font>
            <Font size="11.0" />
         </font></Button>
      <Button fx:id="removerEquipamento" layoutX="86.0" layoutY="113.0" mnemonicParsing="false" onAction="#handleBotaoRemoverEquipamento" text="Remover equipamento cadastrado" />
      <Button fx:id="solicitarReparo" layoutX="90.0" layoutY="146.0" mnemonicParsing="false" onAction="#hadleBotaoSolicitarReparo" text="Solicitar reparo em equipamento" />
      <Button fx:id="voltar" layoutX="14.0" layoutY="1.0" mnemonicParsing="false" onAction="#handleBotaoVoltar" text="Voltar" />
    </children>
</AnchorPane>
