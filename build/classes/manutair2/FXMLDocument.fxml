      <Button fx:id="cliente" layoutX="133.0" layoutY="80.0" mnemonicParsing="false" onAction="#handleBotaoCliente" text="Cliente" />
      <Button fx:id="tecnicodeCampo" layoutX="103.0" layoutY="153.0" mnemonicParsing="false" onAction="#handleBotaoTecnicoCampo" text="Técnico de Campo" />
    </children>
